#include<bits/stdc++.h>
using namespace std;
void display(vector<int> &a){
    int n = a.size();
    for(int i= 0;i<n;i++){
        cout << a[i] << " ";
    }
    cout << endl;

    
}
int main(){
    vector<int> v;
    v.push_back(4);
    v.push_back(5);
    v.push_back(3);
    v.push_back(2);
    v.push_back(1);
    v.push_back(7);
    display(v);

    int i = 0;
    int j = v.size()-1;
    while(i<=j){
        swap(v[i],v[j]);
        i++;
        j--;
        
    }
    display(v);


}
